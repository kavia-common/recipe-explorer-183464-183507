# Example environment variables for Recipe Explorer Frontend (Vite)
# Note: Vite only exposes variables prefixed with VITE_ to the client.
# If you need to access any of these on the client, ensure your code uses import.meta.env.VITE_*
# The orchestrator will manage setting these in the actual .env file.

# Port for the dev server (Vite reads PORT for server.port override via vite.config.js)
PORT=3000

# App URLs and configuration (use VITE_ prefix to expose to client code)
VITE_REACT_APP_API_BASE=
VITE_REACT_APP_BACKEND_URL=
VITE_REACT_APP_FRONTEND_URL=
VITE_REACT_APP_WS_URL=
VITE_REACT_APP_NODE_ENV=development
VITE_REACT_APP_NEXT_TELEMETRY_DISABLED=1
VITE_REACT_APP_ENABLE_SOURCE_MAPS=true
VITE_REACT_APP_PORT=3000
VITE_REACT_APP_TRUST_PROXY=false
VITE_REACT_APP_LOG_LEVEL=info
VITE_REACT_APP_HEALTHCHECK_PATH=/health
VITE_REACT_APP_FEATURE_FLAGS=
VITE_REACT_APP_EXPERIMENTS_ENABLED=false
